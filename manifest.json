{
   "app": {
      "launch": {
         "local_path": "index.html"
      }
   },
   "description": "To test the responsiveness of the web page",
   "permissions": [
		"webRequest",
		"webRequestBlocking",
		"<all_urls>",
		"tabs"
	],
	"background": {
      "scripts": [ "background.js" ]
   },
   "icons": {
      "128": "128x128 .png",
      "16": "16x16.png"
   },
   "manifest_version": 2,
   "name": "Fluid Tester",
   "offline_enabled": true,
   "version": "1.1"
}

